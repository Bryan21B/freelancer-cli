{
  "name": "freelancer-cli",
  "version": "0.0.3",
  "description": "",
  "main": "./dist/src/index.js",
  "bin": "./dist/src/index.js",
  "scripts": {
    "test": "vitest --config ./config/vitest.config.mts",
    "prepare": "husky",
    "fix-link": "pnpm unlink && pnpm link",
    "build": "rm -rf dist && tsc --build --force ",
    "migrate": "pnpm build && npx drizzle-kit generate --config=./config/drizzle.config.ts && npx drizzle-kit migrate --config=./config/drizzle.config.ts",
    "wipe-db": "rm -rf freelancer-cli.db && rm -rf src/db/migrations"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.6.1",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/node": "^22.13.10",
    "@types/validator": "^13.12.2",
    "cz-conventional-changelog": "^3.3.0",
    "drizzle-kit": "^0.30.5",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vitest": "^3.0.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "@libsql/client": "^0.14.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "conf": "^13.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "execa": "^9.5.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sqlite3"
    ],
    "overrides": {}
  },
  "type": "module"
}
